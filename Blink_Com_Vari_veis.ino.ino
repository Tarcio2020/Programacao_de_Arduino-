//****Criar variável tempo 

int tempo;

void loop() {
  pinMode(9, OUTPUT);  //define como a saída 9
  
}

void loop() {
  tempo = 400;
  digitalWrite(9, HIGH);
  delay(tempo);
  digitalWrite(9, LOW);
  delay(tempo);
  

}
